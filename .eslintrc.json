{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    // Eslint rules
    "no-trailing-spaces": "error",
    "camelcase": ["error", {"properties": "always"}],

    // Prettier rules. See more in `.prettierrc`
    "prettier/prettier": "error"
  }
}
